{
  "name": "beer-service",
  "version": "1.0.0",
  "description": "Beer Service",
  "main": "index.ts",
  "license": "MIT",
  "private": true,
  "keywords": [
    "fastify"
  ],
  "scripts": {
    "knex": "knex --knexfile=./src/knexfile.ts init",
    "make": "knex --knexfile=./src/knexfile.ts migrate:make",
    "migrate": "knex --knexfile=./src/knexfile.ts migrate:latest",
    "rollback": "knex --knexfile=./src/knexfile.ts migrate:rollback",
    "coverage": "nyc npm run test",
    "build": "yarn clean && tsc",
    "clean": "rimraf dist",
    "start": "node dist/server.js",
    "start:dev": "nodemon | pino-pretty",
    "lint": "exit 0",
    "test": "TS_NODE_FILES=true mocha -r ts-node/register test/**/*.spec.ts",
    "prettify": "prettier --write ."
  },
  "dependencies": {
    "@fastify/autoload": "^5.4.1",
    "@fastify/compress": "^6.1.1",
    "@fastify/cors": "^8.1.1",
    "@fastify/helmet": "^10.0.2",
    "@fastify/swagger": "^8.1.0",
    "@sentry/node": "^6.19.2",
    "@sentry/tracing": "^6.19.2",
    "bcrypt": "^5.1.0",
    "camelize": "^1.0.1",
    "dotenv": "^10.0.0",
    "fastify": "^4.9.2",
    "fastify-plugin": "^4.3.0",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.95.15",
    "mssql": "6.3.2",
    "pino": "^7.8.0",
    "snakeize": "^0.1.0",
    "tedious": "^15.1.0"
  },
  "devDependencies": {
    "@sinclair/typebox": "^0.23.2",
    "@types/bcrypt": "^5.0.0",
    "@types/chai": "^4.2.22",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/mocha": "^9.0.0",
    "@types/node": "^17.0.7",
    "@types/sinon": "^10.0.11",
    "chai": "^4.3.4",
    "cpy-cli": "^3.1.1",
    "mocha": "^9.1.3",
    "nodemon": "^2.0.15",
    "pino-pretty": "^7.3.0",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "sinon": "^13.0.1",
    "ts-node": "^10.4.0",
    "type-fest": "^2.12.0",
    "typescript": "^4.5.2"
  },
  "engines": {
    "node": ">= 16.13.0",
    "yarn": ">= 1.20.0"
  }
}
